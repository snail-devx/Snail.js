    "merge-source-map": "^1.1.0",
    "postcss": "^8.5.3",
    "postcss-nested": "^7.0.2",
    "postcss-url": "^10.1.3",
    "source-map": "^0.7.4",
    "snail.core": ">=1.2.5",
    "snail.rollup": ">=1.2.2",
    "snail.rollup-asset": ">=1.2.1",
    "snail.rollup-inject": ">=1.2.1",
    "snail.rollup-url": ">=1.2.1"
  },
  "devDependencies": {
    "@types/less": "^3.0.8"
