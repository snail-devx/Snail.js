{
  "name": "snail.rollup-html",
  "description": "配合【snail.rollup】使用的插件，支持组件绑定html文件，并自动将组件js加入html的依赖script标签中",
  "author": "snail_dev@163.com",
  "license": "MIT",
  "version": "1.2.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "types": "tsc -p ./tsconfig.dts.json --declarationDir ./dist/_types"
  },
  "dependencies": {
    "snail.core": ">=1.2.7",
    "snail.rollup": ">=1.2.2"
  }
}