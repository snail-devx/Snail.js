  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.5.13",
    "snail.core": ">=2.0.12",
    "snail.rollup": ">=1.2.8",
    "snail.rollup-script": ">=1.2.4",
    "snail.rollup-style": ">=1.2.8"
  },
  "devDependencies": {
    "vue": "^3.5.13",
    "snail.rollup-inject": ">=1.2.1",
    "snail.rollup-url": ">=1.2.2",
    "snail.rollup-asset": ">=1.2.1"
  }
}