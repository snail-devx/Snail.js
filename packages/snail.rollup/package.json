{
  "name": "snail.rollup",
  "description": "基于Rollup封装的打包工具，基于项目化方式，管理文件依赖边界；脚本输出为独立组件，适合大型Library项目",
  "author": "snail_dev@163.com",
  "license": "MIT",
  "version": "1.2.5",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "types": "tsc -p ./tsconfig.dts.json --declarationDir ./dist/_types"
  },
  "dependencies": {
    "snail.core": ">=1.2.8"
  },
  "devDependencies": {
    "rollup": ">=4.39.0"
  }
}