{
  "name": "snail.rollup-asset",
  "description": "配合【snail.rollup】使用的插件，支持静态资源文件拷贝到输出目录",
  "author": "snail_dev@163.com",
  "license": "MIT",
  "version": "1.2.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "types": "tsc -p ./tsconfig.dts.json --declarationDir ./dist/_types"
  },
  "dependencies": {
    "snail.core": ">=1.2.11",
    "snail.rollup": ">=1.2.8",
    "snail.rollup-url": ">=1.2.1"
  }
}