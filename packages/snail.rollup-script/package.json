{
  "name": "snail.rollup-script",
  "description": "配合【snail.rollup】使用的插件，支持ts、js、cjs、mjs等js脚本文件打包编译",
  "author": "snail_dev@163.com",
  "license": "MIT",
  "version": "1.2.4",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "types": "tsc -p ./tsconfig.dts.json --declarationDir ./dist/_types"
  },
  "dependencies": {
    "vite": "^6.3.0",
    "snail.core": ">=2.0.9",
    "snail.rollup": ">=1.2.8"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5"
  }
}